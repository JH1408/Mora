{
  "name": "flashcard-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:push:dev": "env $(cat .env.local | xargs) prisma db push && env $(cat .env.local | xargs) prisma db seed",
    "db:push:prod": "env $(cat .env | xargs) prisma db push && env $(cat .env | xargs) prisma db seed",
    "db:migrate:dev": "env $(cat .env.local | xargs) prisma migrate dev && env $(cat .env.local | xargs) prisma db seed",
    "db:migrate:prod": "env $(cat .env | xargs) prisma migrate deploy && env $(cat .env | xargs) prisma db seed",
    "db:studio:dev": "env $(cat .env.local | xargs) prisma studio",
    "db:studio:prod": "env $(cat .env | xargs) prisma studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.9.0",
